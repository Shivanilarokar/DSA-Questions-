import random
import time

# Emoji list to use for art
emojis = ["âœ¨", "ğŸŒ¸", "ğŸ”¥", "ğŸ’", "âš¡", "ğŸ®", "ğŸ¦‹", "ğŸŒ™", "ğŸ€", "ğŸ’¥"]

def random_art(width=30, height=10, delay=0.2):
    for _ in range(height):
        row = "".join(random.choice(emojis) for _ in range(width))
        print(row)
        time.sleep(delay)

if __name__ == "__main__":
    print("ğŸ¨ Welcome to Random Emoji Art ğŸ¨\n")
    random_art()
